@model Application.DTOs.DashBoardDTO

<h1>Dashboard</h1>
<form action="ManageDashboard" method="POST">
    <label>Sort</label>
    <select name="sort">
        <option value="1">Desending</option>
        <option value="2">Ascending</option>
    </select>
    <input type="datetime-local" asp-for="DateTo"/>
    <input type="datetime-local" asp-for="DateFrom"/>
    <input type="submit" value="Reports"/>
</form>